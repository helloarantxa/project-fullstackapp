<h2>All Products</h2>

{{#each products}}
<br>
  <div class="product">
    <h3>{{name}}</h3>

    <img class="arrangements" src="{{imageUrl}}" alt="{{name}}">

    <p>{{description}}</p>

  </div>
    <a href="/product/product-details/{{_id}}">Details</a>
  </div>
<br>
{{else}}
  <p>No products found.</p>
{{/each}}